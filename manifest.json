{
    "manifest_version": 3,
    "name": "Notes for TeamConnect",
    "version": "1.0",
    "action": {
      },
      "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_idle",
          "js": ["jquery.min.js", "notes.js"]
          
        }
      ],
      "permissions": [
        "storage"
      ]
  }