{
    "manifest_version": 3,
    "name": "Omnipresent Notes",
    "version": "1.0",
    "action": {
      },
      "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_idle",
            "css":["notes.css"],
          "js": ["jquery.min.js","notes.js"]
          
        }
      ],
      "permissions": [
        "storage"
      ],
    "web_accessible_resources": [
      {
        "resources": [ "minimize.svg","maximize.svg" ],
        "matches": [ "<all_urls>"]
      }
    ]
  }